{
  "$schema": "https://turbo.build/schema.json",

  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "build/**"
      ]
    },
    "cdktf#build": {
      "dependsOn": ["^build"],
      "outputs": [
        "**/*.js",
        "**/*.d.ts"
      ]
    },
    "package": {
      "dependsOn": ["build"],
      "outputs": [
        "dist/**"
      ]
    },
    
    "lint": {
      "outputs": []
    },

    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },

    "test:watch": {
      "cache": false
    },

    "watch": {
      "cache": false
    }
  }
}