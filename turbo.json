{
  "$schema": "https://turbo.build/schema.json",

  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "build/**"
      ]
    },
    "cdktf#build": {
      "dependsOn": ["^build"],
      "outputs": [
        "**/*.js",
        "**/*.d.ts"
      ]
    },
    "package": {
      "dependsOn": ["^package"],
      "outputs": [
        "dist/**"
      ]
    }
  }
}